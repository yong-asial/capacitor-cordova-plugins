{
    "name": "monaca-template-helloworld",
    "version": "4.0.0",
    "displayName": "Monaca Template HelloCordova",
    "engines": {
        "node": ">=18.16"
    },
    "scripts": {
        "monaca:preview": "npm run dev",
        "monaca:trapeze-android": "trapeze run config.yaml -y --android-project android",
        "monaca:trapeze-ios": "trapeze run config.yaml -y --ios-project ios/App",
        "dev": "browser-sync start -s www/ --watch --port 8080 --ui-port 8081"
    },
    "devDependencies": {
        "@capacitor/assets": "3.0.4",
        "@capacitor/cli": "5.7.0",
        "@trapezedev/configure": "7.0.10",
        "browser-sync": "~2.27.7"
    },
    "dependencies": {
        "@capacitor/android": "5.7.0",
        "@capacitor/core": "5.7.0",
        "@capacitor/ios": "5.7.0",
        "cordova-plugin-battery-status": "2.0.3",
        "cordova-plugin-camera": "7.0.0",
        "cordova-plugin-device": "2.1.0",
        "cordova-plugin-dialogs": "2.0.2",
        "cordova-plugin-file": "8.0.1",
        "cordova-plugin-geolocation": "5.0.0",
        "cordova-plugin-media": "7.0.0",
        "cordova-plugin-media-capture": "5.0.0",
        "cordova-plugin-network-information": "3.0.0",
        "cordova-plugin-screen-orientation": "3.0.4",
        "cordova-plugin-statusbar": "4.0.0",
        "cordova-plugin-vibration": "3.1.1",
        "monaca-plugin-monaca-core": "3.3.1"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-battery-status": {},
            "cordova-plugin-camera": {},
            "cordova-plugin-device": {},
            "cordova-plugin-dialogs": {},
            "cordova-plugin-file": {},
            "cordova-plugin-geolocation": {},
            "cordova-plugin-media": {},
            "cordova-plugin-media-capture": {},
            "cordova-plugin-network-information": {},
            "cordova-plugin-screen-orientation": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-vibration": {},
            "monaca-plugin-monaca-core": {}
        }
    }
}
